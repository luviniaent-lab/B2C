{
  "expo": {
    "name": "EventBookingApp",
    "slug": "event-booking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FF4757"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"],
    "plugins": ["expo-router"],
    "scheme": "eventbookingapp"
  }
}